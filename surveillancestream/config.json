{
    "name": "Surveillance stream",
    "version": "0.0.28",
    "slug": "surveillance_stream",
    "description": "Surveillance stream",
    "arch": ["armv7"],
    "startup": "application",
    "boot": "auto",
    "webui": "[PROTO:SSL]://[HOST]:[PORT:3001]/dashboard",
    "ingress": true,
    "ingress_port": "3001",
    "host_network": true,
    "hassio_api": true,
    "hassio_role": "default",
    "map": [
        "share",
        "ssl"
    ],
    "schema": {
        "DATABASE_HOST": "str",
        "DATABASE_PORT": "str",
        "DATABASE_USER": "str",
        "DATABASE_PASS": "str",
        "DATABASE_NAME": "str",
        "SSL": "bool"
    },
    "options": {
        "DATABASE_HOST": "",
        "DATABASE_PORT": "",
        "DATABASE_USER": "",
        "DATABASE_PASS": "",
        "DATABASE_NAME": "",
        "SSL": true
    },
    "ports": {
        "3001/tcp": 3001,
        "3000/tcp": 3000
    },
    "ports_description": {
        "3001/tcp": "Web interface",
        "3000/tcp": "GraphQL server"
    }
  }
